<h1>User activity analytics</h1>
<h3>Show stats for:</h3>
<ul>
  <% @keys.each do |key| %>
    <li>
      <%= link_to key, show_url(key: CGI.escape(key)) %>
    </li>
  <% end %>
</ul>
<h3>Overall activity chart</h3>
<%= line_chart @data %>
<h3>Last 20 actions</h3>
<%= render partial: 'activities'%>
